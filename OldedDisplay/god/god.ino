#include <Wire.h>
#include <Adafruit_GFX.h>
#include <Adafruit_SSD1306.h>

#define SCREEN_WIDTH 128  // OLED display width, in pixels
#define SCREEN_HEIGHT 64  // OLED display height, in pixels
#define OLED_RESET -1     // Reset pin # (or -1 if sharing Arduino reset pin)

Adafruit_SSD1306 display(SCREEN_WIDTH, SCREEN_HEIGHT, &Wire, OLED_RESET);
#define prellZeit 50
#define buttonDown 3
#define selectButton 7


bool taste;
bool tasteAlt;
bool steigFlanke;
bool fallFlanke;
unsigned long letzteFlankenZeit = 0;
unsigned long laengeLauflicht = 5;

unsigned long zeitSchritt = 10 ;
unsigned long letzteZeit;


bool entprelterTaster(int taster) {
  tasteAlt = taste;
  taste = !digitalRead(taster);
  steigFlanke = !tasteAlt && taste;
  fallFlanke = tasteAlt && !taste;

  if (steigFlanke && (millis() >= letzteFlankenZeit + prellZeit)) {
    letzteFlankenZeit = millis();
    return true;
  }

  if (fallFlanke) {
    letzteFlankenZeit = millis();

  }
return false;
}


void setup() {
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
  display.clearDisplay();       // Clear the display buffer
  display.setTextColor(WHITE);  // Set text color to white
  display.setTextSize(1);       // Set text size to 1
  display.setCursor(0, 0);      // Set cursor position to (0,0)
  pinMode(buttonDown, INPUT);
  pinMode(selectButton, INPUT);
  io();
}
int selection;
void loop() {
  switch (selection) {
    case 1:
      fried();
    case 2:
      nici();
    case 3:
      scrolling();

    case 4:
      mill();
    default:
      io();
  }
}




void io() {

  for (;;) {

    if (digitalRead(7) == HIGH) {
      loop();
    }

    display.clearDisplay();
    display.setTextSize(1);
    display.setTextColor(WHITE);
    display.setCursor(1, 20);

    if (selection == 1) {
      display.print("   ");
    }
    display.println("fried");
    if (selection == 2) {
      display.print("   ");
    }
    display.println("nici");
    if (selection == 3) {
      display.print("   ");
    }
    display.println("scroling");
    if (selection == 4) {
      display.print("   ");
    }
    display.println("mill");

    display.display();

    if (entprelterTaster(3) == true) {
      selection++;
      if (selection > 4) {
        selection = 1;
      }
    }
  }
}



void nici() {

  display.clearDisplay();

  display.drawLine(64, 0, 64 + 16, 32, WHITE);
  display.drawLine(64 - 16, 32, 64, 0, WHITE);


  display.drawRect((128 / 2) - (32 / 2), 32, 32, 32, WHITE);
  display.drawRect((128 / 2) - (32 / 2), 32, 32, 32, WHITE);

  display.drawLine(32 + 16, 32, (128 / 2) + 16, 64, WHITE);
  display.drawLine(32 + 16, 64, 64 + 16, 32, WHITE);
  display.display();
  while (entprelterTaster(7) == LOW);
  setup();
}


