  display.display();
  display.startscrollright(0, 7);

while(entprelterTaster(7)==LOW){
  delay(1);
}
  display.stopscroll();
  io();
}




void mill() {
  do {
    int count = millis();
    display.clearDisplay();
    display.setTextSize(4);
    display.setTextColor(WHITE);
    display.setCursor(0, 0);
    display.println(count);
    display.display();
  } while (entprelterTaster(7) == LOW);
  setup();
}

